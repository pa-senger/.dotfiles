Host *
  ServerAliveInterval 15
  ServerAliveCountMax 30
  ServerAliveInterval 600
  IPQoS=throughput

  ControlMaster auto
  ControlPath  ~/.ssh/sockets/%r@%h-%p
  ControlPersist  600

